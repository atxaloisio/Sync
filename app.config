        contract="ClientesCadastroReference.ClientesCadastroSoap" name="ClientesCadastroSoap" />
      <endpoint address="http://app.omie.com.br/api/v1/geral/produtos/"
        binding="basicHttpBinding" bindingConfiguration="ProdutosCadastroSoap"
        contract="ProdutosCadastroReference.ProdutosCadastroSoap" name="ProdutosCadastroSoap" />
      <endpoint address="http://app.omie.com.br/api/v1/geral/cidades/"
        binding="basicHttpBinding" bindingConfiguration="CidadesCadastroSoap"
        contract="CidadesCadastroReference.CidadesCadastroSoap" name="CidadesCadastroSoap" />
      <endpoint address="http://app.omie.com.br/api/v1/geral/prodimp/"
        binding="basicHttpBinding" bindingConfiguration="ProdutosImpostosSoap"
        contract="ProdutosImpostosReference.ProdutosImpostosSoap" name="ProdutosImpostosSoap" />
