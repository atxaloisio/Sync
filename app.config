      <endpoint address="http://app.omie.com.br/api/v1/geral/vendedores/"
        binding="basicHttpBinding" bindingConfiguration="VendedoresCadastroSoap"
        contract="VendedoresCadastroReference.VendedoresCadastroSoap"
        name="VendedoresCadastroSoap" />
      <endpoint address="http://app.omie.com.br/api/v1/geral/familias/"
        binding="basicHttpBinding" bindingConfiguration="FamiliasCadastroSoap"
        contract="FamiliasProdutoReference.FamiliasCadastroSoap" name="FamiliasCadastroSoap" />
      <endpoint address="http://app.omie.com.br/api/v1/geral/empresas/"
        binding="basicHttpBinding" bindingConfiguration="EmpresasCadastroSoap"
        contract="EmpresaCadastroReference.EmpresasCadastroSoap" name="EmpresasCadastroSoap" />
    </client>
  </system.serviceModel>
</configuration>